            </xs:element>
            <xs:element name="CreateProductResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="product" type="types:Product"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="GetProductByIdRequest">
                <xs:complexType>
