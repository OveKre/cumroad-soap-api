<?xml version="1.0" encoding="UTF-8"?>
<definitions xmlns="http://schemas.xmlsoap.org/wsdl/"
             xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/"
             xmlns:tns="http://cumroad.api.soap/service"
             xmlns:types="http://cumroad.api.soap/types"
             targetNamespace="http://cumroad.api.soap/service">

    <types>
        <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
                   targetNamespace="http://cumroad.api.soap/types"
                   elementFormDefault="qualified">

            <!-- Common Types -->
            <xs:simpleType name="UserRole">
                <xs:restriction base="xs:string">
                    <xs:enumeration value="user"/>
                    <xs:enumeration value="admin"/>
                </xs:restriction>
            </xs:simpleType>

            <xs:simpleType name="OrderStatus">
                <xs:restriction base="xs:string">
                    <xs:enumeration value="pending"/>
                    <xs:enumeration value="completed"/>
                    <xs:enumeration value="cancelled"/>
                </xs:restriction>
            </xs:simpleType>

            <!-- User Types -->
            <xs:complexType name="User">
                <xs:sequence>
                    <xs:element name="id" type="xs:int"/>
                    <xs:element name="email" type="xs:string"/>
                    <xs:element name="name" type="xs:string"/>
                    <xs:element name="role" type="types:UserRole"/>
                    <xs:element name="createdAt" type="xs:dateTime"/>
                    <xs:element name="updatedAt" type="xs:dateTime"/>
                </xs:sequence>
            </xs:complexType>

            <xs:complexType name="UserWithToken">
                <xs:complexContent>
                    <xs:extension base="types:User">
                        <xs:sequence>
                            <xs:element name="token" type="xs:string"/>
                        </xs:sequence>
                    </xs:extension>
                </xs:complexContent>
            </xs:complexType>

            <xs:complexType name="UserInput">
                <xs:sequence>
                    <xs:element name="email" type="xs:string"/>
                    <xs:element name="password" type="xs:string"/>
                    <xs:element name="name" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>

            <xs:complexType name="UserUpdateInput">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0"/>
                    <xs:element name="password" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>

            <xs:complexType name="LoginInput">
                <xs:sequence>
                    <xs:element name="email" type="xs:string"/>
                    <xs:element name="password" type="xs:string"/>
                </xs:sequence>
            </xs:complexType>

            <!-- Product Types -->
            <xs:complexType name="Product">
                <xs:sequence>
                    <xs:element name="id" type="xs:int"/>
                    <xs:element name="name" type="xs:string"/>
                    <xs:element name="description" type="xs:string"/>
                    <xs:element name="price" type="xs:double"/>
                    <xs:element name="imageUrl" type="xs:string"/>
                    <xs:element name="userId" type="xs:int"/>
                    <xs:element name="createdAt" type="xs:dateTime"/>
                    <xs:element name="updatedAt" type="xs:dateTime"/>
                </xs:sequence>
            </xs:complexType>

            <xs:complexType name="ProductInput">
                <xs:sequence>
                    <xs:element name="name" type="xs:string"/>
                    <xs:element name="description" type="xs:string" minOccurs="0"/>
                    <xs:element name="price" type="xs:double"/>
                    <xs:element name="imageUrl" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>

            <xs:complexType name="ProductUpdateInput">
                <xs:sequence>
                    <xs:element name="name" type="xs:string" minOccurs="0"/>
                    <xs:element name="description" type="xs:string" minOccurs="0"/>
                    <xs:element name="price" type="xs:double" minOccurs="0"/>
                    <xs:element name="imageUrl" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>

            <!-- Order Types -->
            <xs:complexType name="Order">
                <xs:sequence>
                    <xs:element name="id" type="xs:int"/>
                    <xs:element name="userId" type="xs:int"/>
                    <xs:element name="productId" type="xs:int"/>
                    <xs:element name="quantity" type="xs:int"/>
                    <xs:element name="totalPrice" type="xs:double"/>
                    <xs:element name="status" type="types:OrderStatus"/>
                    <xs:element name="createdAt" type="xs:dateTime"/>
                    <xs:element name="updatedAt" type="xs:dateTime"/>
                    <xs:element name="Product" type="types:Product" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>

            <xs:complexType name="OrderInput">
                <xs:sequence>
                    <xs:element name="productId" type="xs:int"/>
                    <xs:element name="quantity" type="xs:int"/>
                </xs:sequence>
            </xs:complexType>

            <xs:complexType name="OrderUpdateInput">
                <xs:sequence>
                    <xs:element name="quantity" type="xs:int" minOccurs="0"/>
                    <xs:element name="status" type="types:OrderStatus" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>

            <!-- Arrays -->
            <xs:complexType name="UserArray">
                <xs:sequence>
                    <xs:element name="user" type="types:User" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:complexType>

            <xs:complexType name="ProductArray">
                <xs:sequence>
                    <xs:element name="product" type="types:Product" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:complexType>

            <xs:complexType name="OrderArray">
                <xs:sequence>
                    <xs:element name="order" type="types:Order" maxOccurs="unbounded"/>
                </xs:sequence>
            </xs:complexType>

            <!-- SOAP Fault -->
            <xs:complexType name="ServiceFault">
                <xs:sequence>
                    <xs:element name="type" type="xs:string"/>
                    <xs:element name="title" type="xs:string"/>
                    <xs:element name="status" type="xs:int"/>
                    <xs:element name="detail" type="xs:string"/>
                    <xs:element name="instance" type="xs:string" minOccurs="0"/>
                    <xs:element name="code" type="xs:int" minOccurs="0"/>
                    <xs:element name="field" type="xs:string" minOccurs="0"/>
                </xs:sequence>
            </xs:complexType>

