                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="CreateUserResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="user" type="types:User"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="GetUserByIdRequest">
