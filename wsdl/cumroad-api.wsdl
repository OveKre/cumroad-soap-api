                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="UpdateOrderResponse">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="order" type="types:Order"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>

            <xs:element name="DeleteOrderRequest">
