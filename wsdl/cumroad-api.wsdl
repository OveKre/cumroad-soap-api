                    <xs:sequence>
                        <xs:element name="id" type="xs:int"/>
                        <xs:element name="product" type="types:ProductUpdateInput"/>
                        <xs:element name="token" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
