                    <xs:sequence>
                        <xs:element name="id" type="xs:int"/>
                        <xs:element name="token" type="xs:string"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="DeleteOrderResponse">
                <xs:complexType/>
            </xs:element>

            <!-- Fault Element -->
            <xs:element name="ServiceFaultElement" type="types:ServiceFault"/>

        </xs:schema>
    </types>

